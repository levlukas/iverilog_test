#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55e244ca0360 .scope module, "tb" "tb" 2 3;
 .timescale 0 0;
v0x55e244cb1360_0 .var "A", 0 0;
v0x55e244cb1430_0 .var "B", 0 0;
v0x55e244cb1500_0 .net "out", 0 0, v0x55e244ca0740_0;  1 drivers
S_0x55e244ca04f0 .scope module, "dut" "and_gate" 2 9, 3 1 0, S_0x55e244ca0360;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "input_1";
    .port_info 1 /INPUT 1 "input_2";
    .port_info 2 /OUTPUT 1 "and_result";
v0x55e244ca0740_0 .var "and_result", 0 0;
v0x55e244cb1180_0 .net "input_1", 0 0, v0x55e244cb1360_0;  1 drivers
v0x55e244cb1240_0 .net "input_2", 0 0, v0x55e244cb1430_0;  1 drivers
E_0x55e244ca0a10 .event anyedge, v0x55e244cb1180_0, v0x55e244cb1240_0;
    .scope S_0x55e244ca04f0;
T_0 ;
    %wait E_0x55e244ca0a10;
    %load/vec4 v0x55e244cb1180_0;
    %load/vec4 v0x55e244cb1240_0;
    %and;
    %store/vec4 v0x55e244ca0740_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55e244ca0360;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e244cb1360_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e244cb1430_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 15 "$display", "A=%b B=%b Output=%b", v0x55e244cb1360_0, v0x55e244cb1430_0, v0x55e244cb1500_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e244cb1360_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e244cb1430_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 18 "$display", "A=%b B=%b Output=%b", v0x55e244cb1360_0, v0x55e244cb1430_0, v0x55e244cb1500_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e244cb1360_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e244cb1430_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 21 "$display", "A=%b B=%b Output=%b", v0x55e244cb1360_0, v0x55e244cb1430_0, v0x55e244cb1500_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e244cb1360_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e244cb1430_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 24 "$display", "A=%b B=%b Output=%b", v0x55e244cb1360_0, v0x55e244cb1430_0, v0x55e244cb1500_0 {0 0 0};
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
