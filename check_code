#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x58378db82360 .scope module, "tb" "tb" 2 3;
 .timescale 0 0;
v0x58378db92dd0_0 .var "A", 0 0;
v0x58378db92ea0_0 .var "B", 0 0;
v0x58378db92f70_0 .net "out", 0 0, L_0x58378db93070;  1 drivers
S_0x58378db824f0 .scope module, "dut" "and_gate" 2 9, 3 3 0, S_0x58378db82360;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "out";
    .port_info 1 /INPUT 1 "a";
    .port_info 2 /INPUT 1 "b";
L_0x58378db93070 .functor AND 1, v0x58378db92dd0_0, v0x58378db92ea0_0, C4<1>, C4<1>;
v0x58378db82720_0 .net "a", 0 0, v0x58378db92dd0_0;  1 drivers
v0x58378db92bf0_0 .net "b", 0 0, v0x58378db92ea0_0;  1 drivers
v0x58378db92cb0_0 .net "out", 0 0, L_0x58378db93070;  alias, 1 drivers
    .scope S_0x58378db82360;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58378db92dd0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58378db92ea0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 15 "$display", "A=%b B=%b Output=%b", v0x58378db92dd0_0, v0x58378db92ea0_0, v0x58378db92f70_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58378db92dd0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58378db92ea0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 18 "$display", "A=%b B=%b Output=%b", v0x58378db92dd0_0, v0x58378db92ea0_0, v0x58378db92f70_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58378db92dd0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58378db92ea0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 21 "$display", "A=%b B=%b Output=%b", v0x58378db92dd0_0, v0x58378db92ea0_0, v0x58378db92f70_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58378db92dd0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58378db92ea0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 24 "$display", "A=%b B=%b Output=%b", v0x58378db92dd0_0, v0x58378db92ea0_0, v0x58378db92f70_0 {0 0 0};
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
